	<queryString language="SQL">
		<![CDATA[Select p.nombre,
             p.apellido,
             p.fecha_nacimiento as fechanacimiento,
             cliente.telefono,
             p.mail,
             p.dni,             
             FacturaModel.cantidad_solicitada as cantidadsolicitada,
             FacturaModel.fecha,
             producto.descripcion,
             producto.imagen_url as imagenurl,
             producto.precio_unitario as preciounitario,
             local.direccion,
             local.telefono as telefonolocal	 
 From pedido as FacturaModel
inner join cliente 
          on cliente.id = FacturaModel.cliente_id         
		  inner join persona as p on p.id = FacturaModel.cliente_id
          inner join empleado on empleado.id = FacturaModel.solicitador_id
          inner join producto on producto.id = FacturaModel.producto_id
          inner join local on local.id = producto.local_id]]>
	</queryString>
